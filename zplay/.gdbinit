# set sysroot /var/shared/openjdk/build/linux-x86_64-normal-server-slowdebug/jdk/lib/amd64/server/libjvm.debuginfo
# set index-cache enabled on
set history save on  # saves the command history between sessions
set pagination off   # if the screen is too long, you won't need to press y to scroll it
set print pretty on  # displays values of classes, structs etc. nicely
set confirm off      # you won't need to confirm commands
handle SIGSEGV nostop noprint pass